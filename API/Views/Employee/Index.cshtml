@model IEnumerable<Employee> 

<h1>List of Employees</h1>

<table>
    <thead>
        <tr>
            <th>GPN No.</th>
            <th>Contact No.</th>
            <th>Email ID</th>
            <th>Address</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var employee in Model)
        {
            <tr>
                <td>@employee.GPN</td>
                <td>@employee.ContactNumber</td>
                <td>@employee.Email</td>
                <td>@employee.Address</td>
            </tr>
        }
    </tbody>
</table>
